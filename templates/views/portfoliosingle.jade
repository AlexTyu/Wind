extends ../layouts/default

block content
	.verticalL
		a.button.icon-left 
	.verticalR
		a.button.icon-right 
	
	.single-portfolio
		.cover
	
			video#videocover(autoplay='', loop='', poster='54.183.64.164/raw-media/first-frame-screencap', muted='')
				source(src="../files/"+data.portfolio.videocover.filename, type='video/mp4')
		
			img#imagecover(src="../../files/"+data.portfolio.cover.filename)
			
			h1= data.portfolio.name
	
		.projectscroller
			.Description
				.Short
					p=data.portfolio.Short
				
				a.url(href="http://"+data.portfolio.URL)= data.portfolio.URL
				
				p.Objective=data.portfolio.Objective
				
				p.Solution
					data.portfolio.Solution
				
				img.first(src="../../files/"+data.portfolio.image1.filename)
				
				p.imagedescription= data.portfolio.image1description
			
				
